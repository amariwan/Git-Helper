    "version": "0.2.0",
    "configurations": [
        {
            "name": "githelper clone",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "clone"
            ]
        },
        {
            "name": "githelper pull",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "pull"
            ]
        },
        {
            "name": "githelper sync",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "sync"
            ]
        },
        {
            "name": "githelper status",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "status"
            ]
        },
        {
            "name": "githelper run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "run",
                "ls",
                "-l"
            ]
        },
        {
            "name": "githelper dpull",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "dpull"
            ]
        },  
        {
            "name": "githelper dstatus",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-l",
                "4",
                "dstatus"
            ]
        }                
    ]
}